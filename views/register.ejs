<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="/main.css" />
  </head>
  <body>
    <section class="background">
      <div class="nav-container">
        <ul class="nav-list">
          <li class="dropdown">
            <a class="dropbar" href="#">|||||</a>
            <ul class="dropdown-content">
              <li><a href="/">Home</a></li>
              <li><a href="about">About <%=webApp%></a></li>
              <li><a href="login">Login</a></li>
              <li><a href="register">Register with us</a></li>
              <li><a href="logout">Logout</a></li>
            </ul>
          </li>
        </ul>
      </div>
      <div class="label-container">
        <h2 class="label">Welcome to <%=webApp%></h2>
      </div>
      <div class="content-container">
        <div class="chat-container">
          <form class="formRegister" method="POST" action="/registered">
            <p>First name: <input id="first" type="text" name="first" value="first name" /></p>
            <p>Last name: <input id="last" type="text" name="last" value="last name" /></p>
            <p>Email: <input id="email" type="email" name="email" value="email" /></p>
            <p>Username: <input id="username" type="text" name="username" value="username" /></p>
            <p>Password: <input id="password" type="password" name="password" value="password" minlength="8" /></p>
            <input type="submit" value="Register" />
          </form>
        </div>
      </div>
    </section>
  </body>
</html>